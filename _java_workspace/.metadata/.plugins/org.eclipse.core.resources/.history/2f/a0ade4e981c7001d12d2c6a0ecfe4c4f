package day18;

class MyThread2 implements Runnable {

	@Override
	public void run() {
		// 반드시 run()메서드를 구현해야됨
		for(int i = 0; i < 200; i++) {
			System.out.println(i + "번째 " + getName());
		}
		
	}
	
}

public class ThreadEx2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
