package day17;

import java.io.PrintWriter;

public class PrintWriterEx1 {

	public static void main(String[] args) {
		
		/* PrintWriter: 개체의 형식화된 표햔을 텍스트 출력 스트림으로 출력
		 * 
		 * */
		//println, printf, print
		PrintWriter pw = new PrintWriter(System.out);
		
		String str = "Hello";
		pw.print(str);
		
		
	}

}
