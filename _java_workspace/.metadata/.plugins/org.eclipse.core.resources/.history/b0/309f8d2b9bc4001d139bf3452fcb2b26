package day16;

public class Customer {
	private String customer;
	private int price;
	
	void Customer(String a, int b) {
		this.customer = a;
		this.price = b;
	}
	
	public String getCustomer() {
		return customer;
	}

	public void setCustomer(String customer) {
		this.customer = customer;
	}

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}

	@Override
	public String toString() {
		return "Customer [adult=" + adult + ", child=" + child + ", price=" + price + "]";
	}
	
}
